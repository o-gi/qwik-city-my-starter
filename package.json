{
  "name": "qwik-city-my-starter",
  "description": "Qwik City + Radix UI + Tailwind + Vitest + Playwright + Wretch",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "qwik": "qwik",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.18.1",
    "@builder.io/qwik-city": "0.2.1",
    "@builder.io/qwik-react": "0.2.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@playwright/test": "^1.27.1",
    "@types/eslint": "8.21.0",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "autoprefixer": "10.4.11",
    "eslint": "8.33.0",
    "eslint-plugin-qwik": "0.18.1",
    "husky": "^8.0.0",
    "node-fetch": "3.3.0",
    "postcss": "^8.4.16",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.9.5",
    "undici": "5.18.0",
    "vite": "4.1.1",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "^0.25.0"
  },
  "dependencies": {
    "@builder.io/qwik-react": "^0.2.1",
    "@radix-ui/react-avatar": "^1.0.1",
    "wretch": "^2.4.1"
  }
}
